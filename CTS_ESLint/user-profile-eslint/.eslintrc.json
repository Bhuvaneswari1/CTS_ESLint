{
    "env":{
        "browser":true,
        "es2021":true
    },
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react"],
    "rules":{
        "semi":["error","always"],
        "quotes":["error","single"],
        "no-console":"warn",
        "react/prop-types":"error",
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
        "react":{
            "version":"detect"
        }
    }
}